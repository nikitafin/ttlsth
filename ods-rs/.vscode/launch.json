    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch",
            "cwd": "${workspaceFolder}",
            "cargo": {
                "args": [
                    "run",
                    "--bin",
                    "ex11"
                ]
            },
            "args": [
                "<",
                "${workspaceFolder}/data/strings"
            ]
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'ods-rs'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=ods-rs"
                ],
                "filter": {
                    "name": "ods-rs",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'uasge'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=uasge",
                    "--package=ods-rs"
                ],
                "filter": {
                    "name": "uasge",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
